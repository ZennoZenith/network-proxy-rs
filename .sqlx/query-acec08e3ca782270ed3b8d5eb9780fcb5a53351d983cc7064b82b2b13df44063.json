  "query": "SELECT u.user_id, ut.typ, u.name, u.email, pa.pwd, pa.pwd_salt as \"pwd_salt: uuid::Uuid\", pa.token_salt as \"token_salt: uuid::Uuid\"\n            FROM users u\n            INNER JOIN user_type ut ON u.user_type_serial_id = ut.serial_id\n            INNER JOIN password_auth pa ON u.serial_id = pa.user_serial_id\n            WHERE u.email = ? \n            LIMIT 1;",
